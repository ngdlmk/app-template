{
  "name": "mektapp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android:dev": "react-native run-android --variant devDebug --appIdSuffix dev",
    "android:staging": "react-native run-android --variant stagingDebug --appIdSuffix staging",
    "android:prod": "react-native run-android --variant prodDebug --appIdSuffix prod",
    "ios:dev": "react-native run-ios --scheme Mektapp-Dev",
    "ios:staging": "react-native run-ios --scheme Mektapp-Staging",
    "ios:prod": "react-native run-ios --scheme Mektapp",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest"
  },
  "dependencies": {
    "@react-native-firebase/analytics": "^18.8.0",
    "@react-native-firebase/app": "^18.8.0",
    "@react-native-firebase/auth": "^18.8.0",
    "@react-native-firebase/crashlytics": "^18.8.0",
    "@react-native-firebase/messaging": "^18.8.0",
    "react": "18.2.0",
    "react-native": "0.73.3",
    "react-native-config": "^1.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "0.73.20",
    "@react-native/eslint-config": "0.73.2",
    "@react-native/metro-config": "0.73.4",
    "@react-native/typescript-config": "0.73.1",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "18.2.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
